<simulation>
	<integrator_data mode="var_dt" parameter="0.1" time_step="0.1"/>
	<chemistry_data>
		<participating_species>
			<species name="e-" concentration="2500.0e+15"/>
			<species name="Ar" concentration="5000.0e+15"/>
			<species name="Ar+" concentration="0.0"/>
		</participating_species>
		<reactions>
			<reaction name="ionization">
				<rate_constant mode="interpol_k">
					<load>rate_constants.xml</load>
				</rate_constant>
				<educts>
					<stoichiometric_coeff species="e-" value="1.0"/>
					<stoichiometric_coeff species="Ar" value="1.0"/>
					<stoichiometric_coeff species="Ar+" value="0.0"/>
				</educts>
				<products>
					<stoichiometric_coeff species="e-" value="2.0"/>
					<stoichiometric_coeff species="Ar" value="0.0"/>
					<stoichiometric_coeff species="Ar+" value="1.0"/>
				</products>
				<reaction_powers>
					<reaction_power species="e-" value="1.0"/>
					<reaction_power species="Ar" value="1.0"/>
					<reaction_power species="Ar+" value="0.0"/>
				</reaction_powers>
			</reaction>
		</reactions>
	</chemistry_data>
	<thermodynamic_data mode="const_T" temperature="60000"/>
	<abort_criterion mode="var_steps" parameter="0.001"/>
</simulation>
